generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum UserRole {
  USER
  ADMIN
}

model User {
  id            String   @id @default(cuid())
  name          String
  email         String   @unique
  password_hash String
  location      String
  role          UserRole @default(USER)
  createdAt     DateTime @default(now())
}
